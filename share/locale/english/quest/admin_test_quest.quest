                            say_title("Admin Test - Dungeons (2)")
                            say("")
                            local dungeon_choice2 = select("Thunder Mountains", "Cape Dragon Head", "Nephrite Bay", "Spider Dungeon", "Back")
                            
                            if dungeon_choice2 == 1 then
                                pc.warp(843200, 76800)
                                continue = false
                            elseif dungeon_choice2 == 2 then
                                pc.warp(126100, 19800)
                                continue = false
                            elseif dungeon_choice2 == 3 then
                                pc.warp(429300, 93900)
                                continue = false
                            elseif dungeon_choice2 == 4 then
                                pc.warp(59700, 86100)
