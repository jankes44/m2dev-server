                        pc.warp(863900, 246000, 43)
                        continue = false
                    elseif empire_choice == 7 then
                        -- Dungeons
                        say_title("Admin Test - Dungeons")
                        say("")
                        local dungeon_choice = select("Desert", "Yongbi Desert", "Mount Sohan", "Grotto", "Flame Dungeon", "More...")
                        
                        if dungeon_choice == 1 then
                            pc.warp(218000, 168000)
