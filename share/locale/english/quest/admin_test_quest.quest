                    say("")
                    local empire_choice = select("Shinsoo M1", "Shinsoo M2", "Chunjo M1", "Chunjo M2", "Jinno M1", "Jinno M2", "More...")
                    
                    if empire_choice == 1 then
                        -- Shinsoo M1 (map_a1 = index 1) - Base: 409600,896000 + Town: 597,682 * 100
                        pc.warp(469300, 964200, 1)
                        continue = false
                    elseif empire_choice == 2 then
                        -- Shinsoo M2 (map_a3 = index 3) - Base: 307200,819200 + Town: 459,637 * 100
                        pc.warp(353100, 882900, 3)
